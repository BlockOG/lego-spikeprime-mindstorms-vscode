{
	"name": "lego-spikeprime-mindstorms-vscode",
	"displayName": "lego-spikeprime-mindstorms-vscode",
	"description": "Helps you connect Lego's SPIKE Prime/MindStorms Robot Inventor",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:lego-spikeprime-mindstorms-vscode.connectToHub",
		"onCommand:lego-spikeprime-mindstorms-vscode.disconnectFromHub",
		"onCommand:lego-spikeprime-mindstorms-vscode.getStorageStatus"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "lego-spikeprime-mindstorms-vscode.connectToHub",
				"title": "LEGO Hub: Connect to Hub"
			},
			{
				"command": "lego-spikeprime-mindstorms-vscode.disconnectFromHub",
				"title": "LEGO Hub: Disconnect from Hub"
			},
			{
				"command": "lego-spikeprime-mindstorms-vscode.getStorageStatus",
				"title": "LEGO Hub: Get Storage Status"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"postinstall": "electron-rebuild -v 11.3.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/serialport": "^8.0.1",
		"@types/uuid": "^8.3.0",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"electron": "^11.4.2",
		"electron-rebuild": "^2.3.4",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"serialport": "^9.0.7",
		"uuid": "^8.3.2"
	}
}